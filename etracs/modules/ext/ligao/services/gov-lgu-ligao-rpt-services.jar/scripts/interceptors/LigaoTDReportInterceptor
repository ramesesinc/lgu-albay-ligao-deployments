import com.rameses.annotations.*;

class LigaoTDReportInterceptor {

	@ActiveDB("ligao_tdreport") 
	def tdreport; 

	@After( pattern="TDReportService.buildTaxDec", index=100 ) 
	public void afterBuildTaxDec( event ) { 
		def result = event.result; 
		def lands = tdreport.getStandardLandAssessment([ faasid: result.faasid ]); 
		//if ( lands ) result.lands = lands; 
	} 

}